                "rules": [
                    {
                        "description": "Map right_control to mission control when it has been hit twice successively",
                        "type": "basic",
                        "from": {
                            "key_code": "right_control",
                            "modifiers": {
                                "optional": [
                                    "any"
                                ]
                            }
                        },
                        "to": [
                            {
                                "key_code": "mission_control"
                            }
                        ],
                        "conditions": [
                            {
                                "type": "variable_if",
                                "name": "right_control pressed",
                                "value": 1
                            }
                        ]
                    },
                    {
                        "description": "set the 'right_control pressed' viarable used in the double-right_control-press modification above",
                        "type": "basic",
                        "from": {
                            "key_code": "right_control",
                            "modifiers": {
                                "optional": [
                                    "any"
                                ]
                            }
                        },
                        "to": [
                        ],
                        "to_delayed_action": {
                            "to_if_invoked": [
                                {
                                    "set_variable": {
                                        "name": "right_control pressed",
                                        "value": 0
                                    }
                                }
                            ],
                            "to_if_canceled": [
                                {
                                    "set_variable": {
                                        "name": "right_control pressed",
                                        "value": 0
                                    }
                                }
                            ]
                        }
                    },

                    {
                        "description": "Map right_shift to tmux's <CTRL>-a when pressed twice",
                        "type": "basic",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "right_shift"
                                },
                                "to": [
                                    {
                                        "set_variable": {
                                            "name": "right_shift pressed",
                                            "value": 1
                                        }
                                    },
                                    {
                                        "key_code": "right_shift"
                                    }
                                ],
                                "to_delayed_action": {
                                    "to_if_canceled": [
                                        {
                                            "set_variable": {
                                                "name": "right_shift pressed",
                                                "value": 0
                                            }
                                        }
                                    ],
                                    "to_if_invoked": [
                                        {
                                            "set_variable": {
                                                "name": "right_shift pressed",
                                                "value": 0
                                            }
                                        }
                                    ]
                                },
                                "type": "basic"
                            }
                        ]
                    }
                ]
            },
